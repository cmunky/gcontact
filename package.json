{
  "name": "gcontact",
  "version": "1.0.0",
  "description": "Google Contacts Integration",
  "main": "Gruntfile.js",
  "config": {
    "scopes": [
      "https://www.googleapis.com/auth/documents",
      "https://www.googleapis.com/auth/script.send_mail",
      "https://www.googleapis.com/auth/script.storage",
      "https://www.googleapis.com/auth/drive",
      "https://www.google.com/m8/feeds"
    ],
    "credential_dir": "./.credentials",
    "config_dir": "./config",
    "auth_secret": "./config/auth_secret",
    "script_id": "./config/script.id"
  },
  "scripts": {
    "auth": "gapps auth $npm_package_config_auth_secret -b",
    "postinstall": "git clone -b v1-master https://github.com/google/google-api-php-client.git"
  },
  "author": "cmunky",
  "license": "GPL-2.0",
  "dependencies": {
    "google-auth-library": "^0.9.7",
    "googleapis": "^2.1.7",
    "node-google-apps-script": "^1.1.4"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "devDependencies": {
    "grunt": "~0.4.5",
    "grunt-exec": "~0.4.6",
    "grunt-contrib-copy": "^0.8.2",
    "grunt-contrib-jshint": "~0.10.0",
    "grunt-contrib-watch": "~0.6.1",
    "grunt-contrib-nodeunit": "~0.4.1"
  }
}